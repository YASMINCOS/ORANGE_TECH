let resultado = gets(3,5,15,4);
print(FizzBuzz(resultado));

function FizzBuzz(resultado) {
  if (resultado % 3 === 0 && resultado % 5 === 0) {
    return ('FizzBuzz')
  } else if (resultado % 5 === 0) {
    return ('Buzz')
  } else if (resultado % 3 === 0) {
    return ('Fizz')
  } else {
    return resultado
  }
}